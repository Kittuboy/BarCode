{
    "version": 2,
    "name": "qr-barcode-generator",
    "builds": [
        {
            "src": "index.html",
            "src": "style.css",
            "src": "script.js",
            "use": "@vercel/static"
            
        }
    ],
    "routes": [
        {
            "src": "/(.*)",
            "dest": "/index.html"
        }
    ],
    "github": {
        "silent": true
    }
}
